<div class="container" id="container-fix">
  <h1><%= t "allusers" %></h1>
  <%= will_paginate %>
  <ul class="users">
    <% @users.each do |user| %>
      <li>
        <%= user.name %>
        <%= link_to user.email, user_path(user) %>
      </li>
    <% end %>
  </ul>
  <%= will_paginate %>
</div>
